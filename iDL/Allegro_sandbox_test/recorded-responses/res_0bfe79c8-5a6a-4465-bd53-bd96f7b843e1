            if (affiliation) {
                _this.setAffiliationValue(affiliation);
                if (affiliation.indexOf(urlParamsConfiguration_1.urlParamsConfig.affiliation.exception) !== 0) {
                    _this.toggleAffiliationMsg();
                }
            }
            var accountCloningWasCancelled = params[urlParamsConfiguration_1.urlParamsConfig.accountCloningCancelled.key];
            if (accountCloningWasCancelled) {
                _this.fillPreviouslyStoredFormValuesOnAccountCloningCancelled();
            }
            var registrationEntrySource = params[urlParamsConfiguration_1.urlParamsConfig.b2b.key];
            if (registrationEntrySource) {
                _this.setRegistrationEntrySource(registrationEntrySource);
            }
        });
        this.signupService.checkIsCompanyDataDownloadingEnabled().subscribe(function (response) {
            _this.companyDataState = response ? companyDataState_1.CompanyDataState.NotDownloaded : companyDataState_1.CompanyDataState.Disabled;
            _this.isCompanyDataAvailabilityActive = true;
            _this.initialCompanyDataState = _this.companyDataState;
        }, function () {
            _this.companyDataState = companyDataState_1.CompanyDataState.Disabled;
            _this.isCompanyDataAvailabilityActive = true;
            _this.initialCompanyDataState = _this.companyDataState;
        });
    };
