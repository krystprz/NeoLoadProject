    };
    RegistrationComponent.prototype.toggleValidationEmailTaken = function (event, response) {
        if (response.error.errors.length) {
            switch (response.error.errors[0].code) {
                case 'CredentialsValidationErrorSameAsUsername':
                    this.companyForm.get('password').setErrors({ userMessage: response.error.errors[0].userMessage });
                    this.ngaService.sendClickEvent(event, 'CredentialsValidationErrorSameAsUsername', response.error.errors[0].userMessage);
                    this.windowService.scrollTo(0, 0);
                    break;
                case 'EmailTakenByRegisteredCompanyException':
                case 'EmailTakenByRegisteredUserException':
                    this.ngaService.sendClickEvent(event, 'EmailTakenByRegisteredUserException', 'modal.emailReserved', true);
                    this.modal.emailReserved = true;
                    break;
