    RegistrationComponent.prototype.captchaRef = function () {
        return window['grecaptcha']; // tslint:disable-line
    };
    RegistrationComponent.prototype.checkValidationAndDetermineWhetherAllowedToCloneAccount = function (event, taxId, countryCode) {
        var _this = this;
        this.checkValidation(event, taxId, 'taxId', countryCode, function () {
            _this.checkWhetherAllowedToCloneAccount(event, taxId, countryCode);
        });
    };
    RegistrationComponent.prototype.hasBasicDataValidationError = function () {
        return this.companyDataState === companyDataState_1.CompanyDataState.DisabledWithBasicDataValidationError ||
            this.companyDataState === companyDataState_1.CompanyDataState.BasicDataValidationError;
    };
    RegistrationComponent.prototype.shouldDisplayDownloadingCompanyData = function () {
        return (this.countryCode.value === 'PL' || !this.countryCode.value) &&
            this.companyDataState !== companyDataState_1.CompanyDataState.RetryingInProgress &&
            this.companyDataState !== companyDataState_1.CompanyDataState.Disabled &&
            this.companyDataState !== companyDataState_1.CompanyDataState.DisabledWithBasicDataValidationError &&
            !this.isCompanyDataDownloadingFinished &&
            !this.companyDataStateNipAlreadyExists;
    };
    RegistrationComponent.prototype.checkValidation = function (event, val, fieldType, countryCode, actionWhenNoError) {
        var _this = this;
        if (actionWhenNoError === void 0) { actionWhenNoError = function () { }; }
        if (val && val.length) {
            this.signupService.getValidation(val, fieldType, countryCode).subscribe(function () {
                _this[fieldType].setErrors(null);
                actionWhenNoError();
            }, function (response) {
                for (var _i = 0, _a = response.error.errors; _i < _a.length; _i++) {
                    var error = _a[_i];
                    var msgError = error.userMessage ? error.userMessage : error.message;
                    _this[fieldType].setErrors({ userMessage: msgError });
                    _this.ngaService.sendClickEvent(event, fieldType, msgError, val);
                }
            });
        }
    };
    RegistrationComponent.prototype.startCompanyDataDownloadingIfNotToClone = function (event, taxId, countryCode) {
        var _this = this;
        var downloadCompanyDataForNotExistingTaxId = function () {
            _this.setTaxIdDisabled();
            _this.checkAccountCloningEligibilityAndExecute(taxId, countryCode, function () {
                _this.finishCompanyDataDownloading(companyDataState_1.CompanyDataState.TaxIdNumberAlreadyExists);
            }, function () {
                _this.downloadCompanyData(event, taxId, false);
            });
        };
        this.startCompanyDataDownloading(event, taxId, countryCode, downloadCompanyDataForNotExistingTaxId.bind(this));
    };
    RegistrationComponent.prototype.tryAgainDownloadCompanyData = function (event, taxId) {
        this.companyDataState = companyDataState_1.CompanyDataState.RetryingInProgress;
        this.isCompanyDataDownloadingFinished = false;
        this.downloadCompanyData(event, taxId, true);
    };
    RegistrationComponent.prototype.backToBeforeCompanyDataDownloadingStateAndClearCompanyDataAndConsents = function () {
        this.clearCompanyData();
        this.clearConsents();
        this.backToBeforeCompanyDataDownloadingState();
    };
    RegistrationComponent.prototype.preSave = function (event, formData, valid) {
        this.forwardedEvent = event;
        this.formPreSubmitted = true;
        this.formSubmitted = true;
        if (valid) {
            this.isBusy = true;
            this.formData = formData;
            if (this.showCaptcha) {
                this.captchaRef().execute();
            }
            else {
                this.createAccount();
            }
        }
    };
    RegistrationComponent.prototype.createAccount = function () {
        var _this = this;
        this.formData = RegistrationComponent_1.unsetFields(this.formData);
        this.signupService.registerUser(this.formData, this.captcha.model, this.affiliation.value, this.getRegistrationEntrySource()).subscribe(function () {
            _this.ngaService.sendClickEvent(_this.forwardedEvent, 'createAccountFunction', 'success', true, _this.formData.login);
            _this.ngaService.sendClickEvent(_this.forwardedEvent, 'countryOfRegistration', '', _this.countryCode.value);
            _this.fillSuccessRegistrationServiceDataAndNavigate();
        }, function (response) {
            _this.isBusy = false;
            _this.registrationProcess = false;
            if (_this.showCaptcha) {
                _this.captchaRef().reset();
            }
            _this.toggleValidationEmailTaken(_this.forwardedEvent, response);
        });
    };
    RegistrationComponent.prototype.resolved = function (captchaResponse) {
        if (captchaResponse) {
            this.captcha.model = captchaResponse;
        }
        else {
            this.captcha.model = configuration_1.config.captcha.model;
        }
        this.createAccount();
    };
    RegistrationComponent.prototype.matchPassword = function (val) {
        if (val) {
            this.inputPassword.requirements.capitalLetter = new RegExp(/[A-ZĄĆĘŁŃÓŚŻŹА-ЯЁĎŇŤŠČŘŽÝÁÍÉÚŮÄÖÜẞ]+/).test(val);
            this.inputPassword.requirements.smallLetter = new RegExp(/[a-ząćęłńóśźżа-яёěščřžýáíéóúůďťňäöüß]+/).test(val);
            this.inputPassword.requirements.digit = new RegExp(/\d+/).test(val);
            this.inputPassword.requirements.length = val.length >= configuration_1.config.matchPassword.minLength;
        }
    };
    RegistrationComponent.prototype.zipCodeMask = function (e) {
        if (this.countryCode.value === 'PL') {
            if (isNaN(Number(e.key)) || (this.zipCode.value && this.zipCode.value.length >= 6)) {
                e.preventDefault();
            }
            else if (this.zipCode.value && this.zipCode.value.length === 2) {
                this.zipCode.setValue(this.zipCode.value + '-');
            }
        }
    };
    RegistrationComponent.prototype.preventPaste = function (e) {
        e.preventDefault();
    };
    RegistrationComponent.prototype.zipCodeRestrictions = function (e) {
        if (this.countryCode.value === 'PL' && ['Backspace', 'Delete'].includes(e.key)) {
            this.zipCode.setValue('');
        }
    };
    RegistrationComponent.prototype.togglePassword = function () {
        this.inputPassword.type === 'password'
            ? (this.inputPassword.type = 'text')
            : (this.inputPassword.type = 'password');
    };
    RegistrationComponent.prototype.getFilteredStates = function (e) {
        if (e.key === 'ArrowDown' || e.key === 'ArrowUp' || e.key === 'Enter') {
            RegistrationComponent_1.stopDoing(e);
        }
        if (this.countryName && this.countryName.value) {
            this.filteredCountriesBuffer = this.filterData(this.countryName.value);
            if (this.filteredCountriesBuffer.length) {
                this.country.filteredBase = this.filteredCountriesBuffer;
                this.companyForm.get('address.countryName').setErrors(null);
                this.country.showFilterBox = true;
            }
            else {
                this.country.showFilterBox = false;
                this.companyForm.get('address.countryName').setErrors({ countryList: true });
            }
        }
    };
    RegistrationComponent.prototype.keysManager = function (e) {
        if (this.countryName.value !== null && this.filteredCountriesBuffer) {
            var total = this.filteredCountriesBuffer.length;
            switch (e.key) {
                case 'ArrowDown':
                    this.country.selectedIndex = (total + this.country.selectedIndex + 1) % total;
                    this.country.selectedLabel = this.country.filteredBase[this.country.selectedIndex].name[this.languageService.currentLang];
                    RegistrationComponent_1.stopDoing(e);
                    break;
                case 'ArrowUp':
                    this.country.selectedIndex = (total + this.country.selectedIndex - 1) % total;
                    this.country.selectedLabel = this.country.filteredBase[this.country.selectedIndex].name[this.languageService.currentLang];
                    RegistrationComponent_1.stopDoing(e);
                    break;
                case 'Enter':
                    RegistrationComponent_1.stopDoing(e);
                    break;
            }
        }
    };
    RegistrationComponent.prototype.setOtherCountry = function (event, state) {
        this.countryName.setValue(state.name[this.languageService.currentLang]);
        this.countryCode.setValue(state.code);
        this.country.showFilterBox = false;
        this.checkValidation(event, this.phone.value, 'phone', state.code);
        this.ngaService.sendClickEvent(event, 'countryOfRegistration', '', state.code);
    };
    RegistrationComponent.prototype.toggleCountries = function (event) {
        var countryValue = event.target.value;
        if (countryValue === '-1') {
            this.country.showLess = !this.country.showLess;
            this.countryCode.setValue(null);
        }
        else {
            this.checkValidation(event, this.phone.value, 'phone', countryValue);
            this.ngaService.sendClickEvent(event, 'countryOfRegistration', '', countryValue);
        }
        if (countryValue !== 'PL') {
            this.taxId.setValue('');
        }
    };
    RegistrationComponent.prototype.toggleMarketingAgreement = function (type, val, event) {
        this.moreInfo[type] = val;
        this.ngaService.sendClickEvent(event, type, '', val);
        event.preventDefault();
        event.stopPropagation();
    };
    RegistrationComponent.prototype.toggleGdpr = function (type, val) {
        this.moreInfo[type] = val;
    };
    RegistrationComponent.prototype.sendNgaEvent = function (event, val, type) {
        if (val && val.length === 1) {
            this.ngaService.sendClickEvent(event, 'interaction', type);
        }
    };
    RegistrationComponent.prototype.sendSoleTraderSelectedNgaEvent = function (event, legalForm) {
        if (legalForm === '3') {
            this.ngaService.sendClickEvent(event, 'sole_trader', legalForm);
        }
    };
    RegistrationComponent.prototype.toggleAffiliationMsg = function () {
        this.affiliation.visibility = !this.affiliation.visibility;
    };
    RegistrationComponent.prototype.setAffiliationValue = function (affiliation) {
        this.affiliation.value = affiliation;
    };
    RegistrationComponent.prototype.checkBasicValidation = function (field) {
        return (this.formPreSubmitted ||
            field.touched ||
            field.dirty ||
            this.hasBasicDataValidationError());
    };
    RegistrationComponent.prototype.setTaxIdEnabled = function () {
        this.taxIdEnabled = true;
    };
    RegistrationComponent.prototype.setTaxIdDisabled = function () {
        this.taxIdEnabled = false;
    };
    RegistrationComponent.prototype.setRegistrationEntrySource = function (source) {
        var b2bRegistrationEntryPointValue = urlParamsConfiguration_1.urlParamsConfig.b2b.value;
        if (source === b2bRegistrationEntryPointValue) {
            this.registrationEntrySource = registration_source_1.RegistrationSource.ALLEGRO_BUSINESS;
        }
    };
    RegistrationComponent.prototype.getRegistrationEntrySource = function () {
        return this.registrationEntrySource;
    };
    RegistrationComponent.prototype.showStandardRegistrationBanner = function ($event) {
        this.shouldShowFastRegistrationBanner = true;
        this.startCompanyDataDownloading($event, this.taxId.value, this.countryCode.value, this.downloadCompanyData.bind(this));
    };
    RegistrationComponent.prototype.getFormDataForCloneAccountStep = function () {
        return {
            email: this.email.value,
            login: this.login.value,
            phone: this.phone.value
        };
    };
    RegistrationComponent.prototype.getCompanyFormValuesExceptPassword = function () {
        return __assign({}, this.companyForm.value, { password: '' });
    };
    RegistrationComponent.prototype.isEachFormFieldRelatedToAccountCloningWithoutErrors = function () {
        return this.login.errors === null && this.email.errors === null && this.phone.errors === null;
    };
    RegistrationComponent.prototype.checkWhetherAllowedToCloneAccount = function (event, taxId, countryCode) {
        var _this = this;
        if (this.shouldCheckAccountCloningEligibility(taxId, countryCode)) {
            var basicData = new Map([
                ['email', this.email.value],
                ['login', this.login.value],
                ['password', this.password.value],
                ['phone', this.phone.value],
                ['taxId', this.taxId.value]
            ]);
            if (RegistrationComponent_1.isAnyPartOfBasicDataEmpty(basicData, countryCode)) {
                this.companyDataState = companyDataState_1.CompanyDataState.DisabledWithBasicDataValidationError;
                this.sendBasicDataFilledNgaEvent(event, 'error');
                return;
            }
            this.setTaxIdDisabled();
            this.isCheckingEligibilityToCloneAccountInProgress = true;
            this.checkAccountCloningEligibilityAndExecute(taxId, countryCode, function () {
                _this.companyDataState = companyDataState_1.CompanyDataState.TaxIdNumberAlreadyExists;
                _this.isCheckingEligibilityToCloneAccountInProgress = false;
            }, function () {
                _this.isCheckingEligibilityToCloneAccountInProgress = false;
            });
        }
    };
    RegistrationComponent.prototype.shouldCheckAccountCloningEligibility = function (taxId, countryCode) {
        return (this.companyDataState === companyDataState_1.CompanyDataState.Disabled || this.companyDataState === companyDataState_1.CompanyDataState.DisabledWithBasicDataValidationError)
            && countryCode === 'PL' && taxId && taxId.length;
    };
    RegistrationComponent.prototype.startCompanyDataDownloading = function (event, taxId, countryCode, afterMultipleFieldsValidationFunction) {
        var _this = this;
        this.formPreSubmitted = true;
        var basicData = new Map([
            ['email', this.email.value],
            ['login', this.login.value],
            ['password', this.password.value],
            ['phone', this.phone.value],
            ['taxId', this.taxId.value]
        ]);
        if (RegistrationComponent_1.isAnyPartOfBasicDataEmpty(basicData, countryCode)) {
            this.companyDataState = companyDataState_1.CompanyDataState.BasicDataValidationError;
            this.sendBasicDataFilledNgaEvent(event, 'error');
            return;
        }
        this.signupService.getValidationOfMultipleFields(basicData, countryCode).subscribe(function () {
            afterMultipleFieldsValidationFunction(event, taxId, false);
        }, function (response) {
            _this.companyDataState = companyDataState_1.CompanyDataState.BasicDataValidationError;
            _this.sendBasicDataFilledNgaEvent(event, 'error');
            _this.displayFirstFormPartValidationErrors(response);
        });
    };
    RegistrationComponent.prototype.downloadCompanyData = function (event, taxId, isRetry) {
        var _this = this;
        this.signupService.downloadCompanyData(taxId).subscribe(function (response) {
            if (configuration_1.config.allowedLegalForms.includes(response.legalForm)) {
                _this.finishCompanyDataDownloading(companyDataState_1.CompanyDataState.DownloadingSuccess);
                _this.fillCompanyData(event, response);
                _this.sendDataAutoCompletedNgaEvent(event, response);
                _this.sendRetryDownloadCompanyDataNgaEvent(event, isRetry, true);
                _this.sendBasicDataFilledNgaEvent(event, 'true', isRetry);
            }
            else {
                _this.finishCompanyDataDownloading(companyDataState_1.CompanyDataState.NotFound);
                _this.sendRetryDownloadCompanyDataNgaEvent(event, isRetry, false);
                _this.sendBasicDataFilledNgaEvent(event, 'false', isRetry);
            }
        }, function (response) {
            if (response.status === 404) {
                _this.finishCompanyDataDownloading(companyDataState_1.CompanyDataState.NotFound);
            }
            else {
                _this.finishCompanyDataDownloading(companyDataState_1.CompanyDataState.DownloadingFail);
            }
            _this.sendRetryDownloadCompanyDataNgaEvent(event, isRetry, false);
            _this.sendBasicDataFilledNgaEvent(event, 'false', isRetry);
        });
    };
    RegistrationComponent.prototype.checkAccountCloningEligibilityAndExecute = function (taxId, countryCode, actionForExistingTaxId, actionForNotExistingTaxId) {
        this.companyOnboardingCoordinatorService.eligibleToClone(taxId, countryCode).subscribe(function (taxIdExists) {
            if (taxIdExists) {
                actionForExistingTaxId();
            }
            else {
                actionForNotExistingTaxId();
            }
        });
    };
    RegistrationComponent.prototype.finishCompanyDataDownloading = function (companyDataState) {
        this.companyDataState = companyDataState;
        this.isCompanyDataDownloadingFinished = true;
    };
    RegistrationComponent.isAnyPartOfBasicDataEmpty = function (dataFromFirstFormPart, countryCode) {
        return (Array.from(dataFromFirstFormPart.values()).some(function (value) { return !value || !value.trim(); }) ||
            !countryCode ||
            !countryCode.trim());
    };
    RegistrationComponent.prototype.displayFirstFormPartValidationErrors = function (response) {
        for (var _i = 0, _a = response.error.errors; _i < _a.length; _i++) {
            var error = _a[_i];
            this.companyForm.get(error.path).setErrors({ userMessage: error.userMessage });
        }
    };
    RegistrationComponent.prototype.sendBasicDataFilledNgaEvent = function (event, label, isRetry) {
        if (isRetry === void 0) { isRetry = false; }
        if (isRetry) {
            return;
        }
        this.ngaService.sendClickEvent(event, label, '');
    };
    RegistrationComponent.prototype.fillCompanyData = function (event, companyData) {
        this.legalForm = companyData.legalForm.toString();
        this.sendSoleTraderSelectedNgaEvent(event, this.legalForm.value);
        this.firstName = this.capitalizeName(companyData.firstName);
        this.lastName = this.capitalizeName(companyData.lastName);
        this.name = companyData.name;
        this.companyRegister = companyData.companyRegister;
        if (companyData.address) {
            this.addressLine = companyData.address.addressLine;
            this.zipCode = RegistrationComponent_1.fillZipCode(companyData.address.zipCode);
            this.city = companyData.address.city;
            this.state = companyData.address.state;
        }
    };
    RegistrationComponent.prototype.clearCompanyData = function () {
        this.legalForm.reset();
        this.firstName.reset();
        this.lastName.reset();
        this.name.reset();
        this.companyRegister.reset();
        this.addressLine.reset();
        this.zipCode.reset();
        this.city.reset();
        this.state.reset();
    };
    RegistrationComponent.prototype.clearConsents = function () {
        this.companyForm.get('consents').reset();
        this.agreementTerms.reset();
    };
    RegistrationComponent.prototype.capitalizeName = function (name) {
        if (name === null) {
            return '';
        }
        name = name.charAt(0).toUpperCase() + name.slice(1).toLowerCase();
        for (var i = 0; i < name.length; i++) {
            if (name.charAt(i) === '-' || name.charAt(i) === ' ') {
                name = name.slice(0, i + 1) + name.charAt(i + 1).toUpperCase() + name.slice(i + 2, name.length);
            }
        }
        return name;
    };
    RegistrationComponent.prototype.sendDataAutoCompletedNgaEvent = function (event, companyData) {
        var label = RegistrationComponent_1.isCompanyDataCompleted(companyData) ? 'all' : 'part';
        this.ngaService.sendClickEvent(event, label, '');
    };
    RegistrationComponent.prototype.sendRetryDownloadCompanyDataNgaEvent = function (event, isRetry, isSuccessful) {
        if (isRetry) {
            var label = isSuccessful ? 'true' : 'false';
            this.ngaService.sendClickEvent(event, label, '');
        }
    };
    RegistrationComponent.isCompanyDataCompleted = function (companyData) {
        return (companyData &&
            companyData.legalForm &&
            companyData.firstName &&
